@using EntityLayer.Concrete
@model List<Job>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_UILayout.cshtml";
}



<table class="table table-bordered">
    <tr>
        <td>#</td>
        <td>Meslek Adı</td>
        <td>Sil</td>
        <td>Güncelle</td>
        <td>Detaylar</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.JobID</td>
            <td>@item.Name</td>
            <td><a href="/Job/DeleteJob/@item.JobID" class="btn btn-outline-danger">Sil</a></td>
            <td><a href="/Job/UpdateJob/@item.JobID" class="btn btn-outline-success">Güncelle</a></td>
            <td><a href="/Job/DetailsJob/@item.JobID" class="btn btn-outline-primary">Detaylar</a></td>

        </tr>
    }



</table>

